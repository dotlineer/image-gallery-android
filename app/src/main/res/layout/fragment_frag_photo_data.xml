<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context=".fragPhotoData" android:id="@+id/fl_photo_data">

    <!-- TODO: Update blank fragment layout -->


    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/cl_photo_data">

        <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_title_data" app:layout_constraintStart_toEndOf="@+id/tv_title"
                android:layout_marginStart="16dp"
                app:layout_constraintTop_toTopOf="@+id/tv_title"/>
        <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_date_data" app:layout_constraintStart_toEndOf="@+id/tv_date"
                android:layout_marginStart="16dp"
                app:layout_constraintTop_toTopOf="@+id/tv_date"/>
        <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_tags_data" app:layout_constraintStart_toEndOf="@+id/tv_tags"
                android:layout_marginStart="16dp"
                app:layout_constraintTop_toTopOf="@+id/tv_tags"/>
        <TextView
                android:text="TextView"
                android:layout_width="59dp"
                android:layout_height="18dp"
                android:id="@+id/tv_title" android:layout_marginTop="88dp"
                app:layout_constraintTop_toTopOf="parent"
                android:padding="40dp"
                app:layout_constraintLeft_toRightOf="parent"/>
        <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:layout_editor_absoluteX="31dp" android:id="@+id/tv_date" android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/tv_title"/>
        <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:layout_editor_absoluteX="31dp" android:id="@+id/tv_tags" android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/tv_date"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>